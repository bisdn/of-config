<capable-switch xmlns="urn:onf:of111:config:yang"
	xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
	<id>capable-switch-0</id>
	<logical-switches>
		<switch>
			<id>logic-switch-1</id>
			<resources>
				<port nc:operation="delete">veth0</port>
				<port nc:operation="delete">veth2</port>
				<port nc:operation="create">veth4</port>
				<port nc:operation="create">veth6</port>
			</resources>
		</switch>
		<switch>
			<id>logic-switch-2</id>
			<resources>
				<port nc:operation="create">veth0</port>
				<port nc:operation="create">veth2</port>
				<port nc:operation="delete">veth4</port>
				<port nc:operation="delete">veth6</port>
			</resources>
		</switch>
	</logical-switches>
</capable-switch>