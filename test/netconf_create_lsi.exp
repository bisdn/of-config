#!/bin/expect


source netconf.tcl

netconf_connect

set f [open "./xml/edit-config_create_lsi.xml"]
set netconf_xml [read $f]
close $f

send "edit-config --error rollback running\r"
expect {
	"Type the edit configuration data (close editor by Ctrl-D):"	{  }
	timeout 	{ exit 1 }
}

send "$netconf_xml"
send "\x0004"

expect {
	"Result OK"	{  }
	timeout		{ exit 1 }
}

netconf_disconnect